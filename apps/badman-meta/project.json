{
  "name": "badman-meta",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/badman-meta/src",
  "prefix": "badman",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": [
        "{options.outputPath}",
        "dist/apps/badman-meta/.nitro",
        "dist/apps/badman-meta/ssr",
        "dist/apps/badman-meta/analog"
      ],
      "options": {
        "main": "apps/badman-meta/src/main.ts",
        "configFile": "apps/badman-meta/vite.config.ts",
        "outputPath": "dist/apps/badman-meta/client"
      },
      "defaultConfiguration": "production",
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "sourcemap": false,
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "badman-meta:build",
        "port": 4200
      },
      "configurations": {
        "development": {
          "buildTarget": "badman-meta:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "badman-meta:build:production"
        }
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "badman-meta:build"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "apps/badman-meta/**/*.ts",
          "apps/badman-meta/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["apps/badman-meta/coverage"]
    }
  },
  "tags": []
}
