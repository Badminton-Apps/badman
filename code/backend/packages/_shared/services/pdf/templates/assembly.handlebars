<html>

<head>
  <meta charset="UTF-8" />
  <style>
    .wrapper {
      display: -ms-grid;
      display: grid;
      -ms-grid-columns: 1fr 35px 5fr 1.2fr 2fr 2fr 2fr 2fr 2fr;
      grid-template-columns: 1fr 35px 5fr 1.2fr 2fr 2fr 2fr 2fr 2fr;
      border-left: 1px solid black;
    }

    .wrapper {
      border-top: 1px solid black;
    }


    .box {
      border-bottom: 1px solid black;
      border-right: 1px solid black;
      padding: 5px;
    }

    .players {
      border-right: 1px solid black;
      border-bottom: 1px solid black;
    }

    .title {
      -ms-grid-row: 1;
      grid-row: 1;
    }

    .title.type {
      grid-column: 1/3;

    }

    .d0.type {
      -ms-grid-row: 2;
      -ms-grid-row-span: 2;
      grid-row: 2 / 4;
    }

    .d1.type {
      -ms-grid-row: 4;
      -ms-grid-row-span: 2;
      grid-row: 4 / 6;
    }

    .d2.type {
      -ms-grid-row: 6;
      -ms-grid-row-span: 2;
      grid-row: 6 / 8;
    }

    .d3.type {
      -ms-grid-row: 8;
      -ms-grid-row-span: 2;
      grid-row: 8 / 10;
    }

    .s0 {
      border-top: 2px solid black;
    }

    .s0.type {
      -ms-grid-row: 10;
      grid-row: 10;
    }

    .s1.type {
      -ms-grid-row: 11;
      grid-row: 11;
    }

    .s2.type {
      -ms-grid-row: 12;
      grid-row: 12;
    }

    .s3.type {
      -ms-grid-row: 13;
      grid-row: 13;
    }

    .type {
      display: -ms-grid;
      display: grid;
      -ms-grid-column: 1;
      grid-column: 1;
      justify-items: center;
      -webkit-box-align: center;
      -ms-flex-align: center;
      align-items: center;
    }

    .players {
      display: -ms-grid;
      display: grid;
      -ms-grid-column: 2;
      grid-column: 2;
      -ms-grid-row: 2;
      -ms-grid-row-span: 12;
      grid-row: 2 / 14;
      word-break: break-all;
      padding: 0 10px;
      text-transform: uppercase;
      font-size: large;
      justify-items: center;
      -webkit-box-align: center;
      -ms-flex-align: center;
      align-items: center;
      text-align: center;
    }

    .subtitudes .type {
      grid-row: 2 / 6;
      grid-column: 1 / 3;
    }

    .subtitudes .level {
      display: flex;
    }

    .subtitudes .level div {
      flex: 1;
    }

    .subtitudes .pre {
      grid-column: 1 / 4;
    }

    .subtitudes .post {
      grid-column: 5 / 10;
    }

    .name {
      -ms-grid-column: 3;
      grid-column: 3;
    }

    .level {
      -ms-grid-column: 4;
      grid-column: 4;
      text-align: center;
    }

    .number {
      -ms-grid-column: 5;
      grid-column: 5;
    }

    .sum {
      -ms-grid-column: 6;
      grid-column: 6;
    }

    .highest {
      -ms-grid-column: 7;
      grid-column: 7;
    }

    .team {
      -ms-grid-column: 8;
      grid-column: 8;
    }

    .base {
      -ms-grid-column: 9;
      grid-column: 9;
    }
  </style>
</head>

<body>
  <div class="info">
    <div class="header">
      <div class="title">
        Ploegopstellingsformulier â€“ Interclubcompetitie
      </div>
    </div>

    <div>

    </div>

    <div class="info">
      <div class="draw">1e Prov heren B</div>
      <div class="date">{{date}}</div>
    </div>

    <div class="indexes">
      <div class="base-index">Basisspelers: {{baseIndex}}</div>
      <div class="team-index">Titularissen: {{teamIndex}}</div>
    </div>

    <div class="teams">
      <div class="home">Thuisploeg: {{homeTeam}}</div>
      <div class="away">Bezoekers: {{awayTeam}}</div>
    </div>

    <div class="captain">
      Ploegkapitein: {{captain}}
    </div>
  </div>

  <div class="wrapper">
    <div class="box title type"></div>
    <div class="box title name">
      name
    </div>
    <div class="box title level">
      level
    </div>
    <div class="box title number">
      number
    </div>
    <div class="box title sum">
      Som Index
    </div>
    <div class="box title highest">
      Hoogste klas
    </div>
    <div class="box title team">
      Ploeg Index
    </div>
    <div class="box title base">
      Basis Speler
    </div>
    <div class="players">
      t i t u l a r i s s e n
    </div>


    {{#each doubles as |double index|}}
    <div class="box d{{@index}} type">
      {{labelDouble @index ../type}}
    </div>
    <div class="box d{{@index}} p1 name">
      {{double.player1.fullName}}
    </div>
    <div class="box d{{@index}} p1 level">
      {{#if (and (eq type "MX") (gt @index 1))}}
      {{double.player1.lastRankingPlace.mix}}
      {{else}}
      {{double.player1.lastRankingPlace.double}}
      {{/if}}
    </div>
    <div class="box d{{@index}} p1 number">
      {{double.player1.memberId}}
    </div>
    <div class="box d{{@index}} p1 sum">
      {{double.player1.sum}}
    </div>
    <div class="box d{{@index}} p1 highest">
      {{double.player1.highest}}

    </div>
    <div class="box d{{@index}} p1 team">
      {{#if double.player1.team}}
      x
      {{/if}}
    </div>
    <div class="box d{{@index}} p1 base">
      {{#if double.player1.base}}
      x
      {{/if}}
    </div>
    <div class="box d{{@index}} p2 name">
      {{double.player2.fullName}}
    </div>
    <div class="box d{{@index}} p2 level">
      {{#if (and (eq type "MX") (gt @index 1))}}
      {{double.player2.lastRankingPlace.mix}}
      {{else}}
      {{double.player2.lastRankingPlace.double}}
      {{/if}}
    </div>
    <div class="box d{{@index}} p2 number">
      {{double.player2.memberId}}
    </div>
    <div class="box d{{@index}} p2 sum">
      {{double.player2.sum}}

    </div>
    <div class="box d{{@index}} p2 highest">
      {{double.player2.highest}}
    </div>
    <div class="box d{{@index}} p2 team">
      {{#if double.player2.team}}
      x
      {{/if}}
    </div>
    <div class="box d{{@index}} p2 base">
      {{#if double.player2.base}}
      x
      {{/if}}
    </div>
    {{/each}}

    {{#each singles as |single|}}
    <div class="box s{{@index}} type">
      {{labelSingle @index ../type}}
    </div>
    <div class="box s{{@index}} p1 name">
      {{single.fullName}}
    </div>
    <div class="box s{{@index}} p1 level">
      {{single.lastRankingPlace.single}}
    </div>
    <div class="box s{{@index}} p1 number">
      {{single.memberId}}
    </div>
    <div class="box s{{@index}} p1 sum">
      {{single.sum}}
    </div>
    <div class="box s{{@index}} p1 highest">
      {{single.highest}}
    </div>
    <div class="box s{{@index}} p1 team">
      {{#if single.team}}
      x
      {{/if}}
    </div>
    <div class="box s{{@index}} p1 base">
      {{#if single.base}}
      x
      {{/if}}
    </div>
    {{/each}}
  </div>


  <div class="wrapper subtitudes">
    <div class="box pre"></div>
    <div class="box title level">
      <div class="single">S</div>
      <div class="double">D</div>
      <div class="mix">G</div>
    </div>
    <div class="box post"></div>


    {{#each subtitudes as |subtitude|}}
    <div class="box type">
      invallers
    </div>
    <div class="box p{{@index}} name">
      {{subtitude.fullName}}
    </div>
    <div class="box p{{@index}} level">
      <div>{{subtitude.lastRankingPlace.single}}</div>
      <div>{{subtitude.lastRankingPlace.double}}</div>
      <div>{{subtitude.lastRankingPlace.mix}}</div>
    </div>
    <div class="box p{{@index}} number">
      {{subtitude.memberId}}
    </div>
    <div class="box p{{@index}} sum">
      {{subtitude.sum}}
    </div>
    <div class="box p{{@index}} highest">
      {{subtitude.highest}}
    </div>
    <div class="box p{{@index}} team">
      {{#if subtitude.team}}
      x
      {{/if}}
    </div>
    <div class="box p{{@index}} base">
      {{#if subtitude.base}}
      x
      {{/if}}
    </div>
    {{/each}}
  </div>

</body>

</html>