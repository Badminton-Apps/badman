{
  "name": "@badvlasim/import",
  "description": "Ranking simulation import Badminton Vlaanderen",
  "version": "4.18.1",
  "private": true,
  "dependencies": {
    "@badvlasim/shared": "^4.18.1",
    "@fast-csv/parse": "^4.3.2",
    "@types/multer": "^1.4.4",
    "multer": "^1.4.2"
  },
  "scripts": {
    "start": "node -r ts-node/register ./src/server.ts",
    "build": "yarn run clean && yarn run compile",
    "clean": "rimraf -rf ./dist ../_shared/dist",
    "compile": "tsc -b tsconfig.build.json",
    "start:inspect": "node --max-old-space-size=4096  --inspect=6858 -r ts-node/register ./src/server.ts",
    "start:watch": "nodemon --max-old-space-size=4096",
    "start:watch:inspect": "nodemon --max-old-space-size=4096 --exec node --inspect=6858 -r ts-node/register ./src/server.ts",
    "test:local": "jest --colors --watchAll --runInBand",
    "test": "jest --runInBand",
    "lint": "eslint -c ../../.eslintrc ./**/*{.ts,.tsx}",
    "format": "eslint --fix -c ../../.eslintrc ./**/*{.ts,.tsx}"
  },
  "peerDependencies": {
    "moment-timezone": "^0.5.33",
    "sequelize": "^6.5.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testTimeout": 20000,
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "collectCoverage": true,
    "setupFiles": [
      "<rootDir>/../../test/env.js"
    ],
    "reporters": [
      "default",
      "jest-github-actions-reporter"
    ],
    "testEnvironment": "node",
    "coverageReporters": [
      "text-summary",
      "lcov"
    ],
    "testLocationInResults": true
  },
  "devDependencies": {
    "mock-fs": "^5.1.2"
  }
}
