{
  "name": "@badman/frontend-shared",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/frontend/shared/src",
  "prefix": "badman",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/libs/frontend/shared"],
      "options": {
        "project": "libs/frontend/shared/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/frontend/shared/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/frontend/shared/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/frontend/shared"],
      "options": {
        "jestConfig": "libs/frontend/shared/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/frontend/shared/**/*.ts",
          "libs/frontend/shared/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
