{
  "root": "libs/api/simulate",
  "sourceRoot": "libs/api/simulate/src",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/api/simulate/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/api/simulate"],
      "options": {
        "jestConfig": "libs/api/simulate/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
