{
  "name": "backend-pdf",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/backend/pdf/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/backend/pdf",
        "tsConfig": "libs/backend/pdf/tsconfig.lib.json",
        "packageJson": "libs/backend/pdf/package.json",
        "main": "libs/backend/pdf/src/index.ts"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/backend/pdf/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/backend/pdf"],
      "options": {
        "jestConfig": "libs/backend/pdf/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
