{
  "name": "backend-assembly",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/backend/assembly/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/backend/assembly",
        "tsConfig": "libs/backend/assembly/tsconfig.lib.json",
        "packageJson": "libs/backend/assembly/package.json",
        "main": "libs/backend/assembly/src/index.ts",
        "assets": [
          "libs/backend/assembly/*.md"
        ]
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node tools/scripts/publish.mjs backend-assembly {args.ver} {args.tag}"
      },
      "dependsOn": [
        "build"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/backend/assembly/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/backend/assembly/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
