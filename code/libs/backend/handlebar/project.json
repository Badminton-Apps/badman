{
  "name": "backend-handlebar",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/backend/handlebar/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/backend/handlebar",
        "tsConfig": "libs/backend/handlebar/tsconfig.lib.json",
        "packageJson": "libs/backend/handlebar/package.json",
        "main": "libs/backend/handlebar/src/index.ts",
        "assets": [
          "libs/backend/handlebar/src/assets/**/*"
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/backend/handlebar/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/libs/backend/handlebar"
      ],
      "options": {
        "jestConfig": "libs/backend/handlebar/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
