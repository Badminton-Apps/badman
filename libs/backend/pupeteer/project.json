{
  "name": "backend-pupeteer",
  "$schema": "..\\..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "sourceRoot": "libs/backend/pupeteer/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/backend/puppeteer",
        "tsConfig": "libs/backend/puppeteer/tsconfig.lib.json",
        "packageJson": "libs/backend/puppeteer/package.json",
        "main": "libs/backend/puppeteer/src/index.ts",
        "assets": ["libs/backend-puppeteer/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/backend/pupeteer/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/backend/pupeteer"],
      "options": {
        "jestConfig": "libs/backend/puppeteer/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
